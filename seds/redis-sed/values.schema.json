{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "required": [
    "redis"
  ],
  "properties": {
    "mongodb": {
      "type": "object",
      "required": [
        "sed"
      ],
      "properties": {
        "sed": {
          "type": "object",
          "required": [
            "hostname",
            "password"
          ],
          "properties": {
            "hostname": {
              "type": "string",
              "pattern": "^[a-z0-9-_./]+$"
            },
            "password": {
              "type": "string",
              "pattern": "^[A-Za-z0-9-_./]+$"
            }
          }
        }
      }
    }
  }
}